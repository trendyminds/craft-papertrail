{
    "name": "trendyminds/craft-paper-trail",
    "description": "A plugin to connect Craft logs to Papertrail.",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft papertrail"
    ],
    "support": {
        "docs": "https://github.com/trendyminds/craft-papertrail/blob/main/README.md",
        "issues": "https://github.com/trendyminds/craft-papertrail/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "TrendyMinds",
            "homepage": "https://trendyminds.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "rekurzia/yii2-papertrail-log-target": "^0.10.0"
    },
    "autoload": {
        "psr-4": {
          "trendyminds\\craftpapertrail\\": "src/"
        }
    },
    "extra": {
        "name": "Craft PaperTrail",
        "handle": "craft-paper-trail",
        "developer": "TrendyMinds",
        "developerUrl": "https://trendyminds.com",
        "documentationUrl": "https://github.com/trendyminds/craft-papertrail/blob/main/README.md",
        "class": "trendyminds\\craftpapertrail\\CraftPapertrail"
    }
}
